table barang {
  id_barang smallint [pk]
  nama_barang varchar
  harga_beli float
  harga_jual float
  jumlah_barang int
  satuan_barang varchar
}

table transaksi {
 id_transaksi int
 id_barang int
 id_pembeli int
 tanggal date
 keterangan varchar
} 

table keuntungan{
  tanggal date
  keuntungan_harian varchar
}

table keranjang {
  id varchar
  password varchar
}
